{% extends 'base.html.twig' %} {% block title %}Test index{% endblock %} {% block body %}
<div class="d-flex my-3" style="justify-content: space-between;">
    <h1>Test index</h1>
    <a href="/logout">Logout</a>
    <a href="/admin/user/">Manage Users</a>
    <a class="btn btn-primary me-2 pt-2" href="/logout">Logout</a>
</div>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
            <th>actions</th>
        </tr>
    </thead>
    <tbody>
        {% for test in tests %}
        <tr>
            <td>{{ test.id }}</td>
            <td>{{ test.name }}</td>
            <td>{{ test.price }}</td>
            <td>
                <a href="{{ path('app_test_show', {'id': test.id}) }}">show</a>
                <a href="{{ path('app_test_edit', {'id': test.id}) }}">edit</a>
            </td>
        </tr>
        {% else %}
        <tr>
            <td colspan="4">no records found</td>
        </tr>
        {% endfor %}
    </tbody>
</table>

<a class="btn btn-primary" href="{{ path('app_test_new') }}">Create new</a> {% endblock %}